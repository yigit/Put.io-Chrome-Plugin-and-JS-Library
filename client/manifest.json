{
  "name": "Add 2 Put.io",
  "version": "0.4",
  "description": "One click add for torrents to put io from any page",
  "update_url": "http://github.com/downloads/yigit/Put.io-Chrome-Plugin-and-JS-Library/updates.xml",
  "browser_action": {
    "default_icon": "img/icon.png",
    "popup" : "popup.html"
  },
  "permissions": [
    "http://*/", "tabs"
  ],
  "content_scripts": [
  {
      "matches": ["http://*/*", "https://put.io/account/settings"],
      "js": ["js/content.js", "js/prototype.js"]
  }
  ],
  "options_page": "options.html"
}